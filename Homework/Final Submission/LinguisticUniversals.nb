  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"WordsTranslatedTo", "[", "targetLangIdx_Integer", "]"}], " ", ":=",
      " ", 
     RowBox[{"Normal", " ", "@", "  ", 
      RowBox[{"Keys", "[", 
       RowBox[{
        RowBox[{"Query", "[", 
         RowBox[{
         "langsAll", "\[LeftDoubleBracket]", "targetLangIdx", 
          "\[RightDoubleBracket]"}], "]"}], " ", "@", " ", "dataSwadesh"}], 
       "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsTranslatedToBoth", "[", 
      RowBox[{"targetLangAIdx_Integer", ",", " ", "targetLangBIdx_Integer"}], 
      "]"}], " ", ":=", " ", 
     RowBox[{"Normal", " ", "@", " ", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"Intersection", "[", 
         RowBox[{
          RowBox[{"WordsTranslatedTo", "[", "targetLangAIdx", "]"}], ",", " ", 
          RowBox[{"WordsTranslatedTo", "[", "targetLangBIdx", "]"}]}], "]"}], 
        ",", " ", 
        RowBox[{"UpTo", "[", "50", "]"}]}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsTranslateTo", "[", 
      RowBox[{"wordsInEng_List", ",", " ", "targetLangIdx_Integer"}], "]"}], 
     " ", ":=", " ", 
     RowBox[{"Normal", " ", "@", "  ", 
      RowBox[{"Values", "[", 
       RowBox[{
        RowBox[{"Query", "[", 
         RowBox[{"targetLangIdx", ",", " ", "wordsInEng"}], "]"}], " ", "@", 
        " ", "dataSwadesh"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsLimit", "[", "wordsInLang_List", "]"}], " ", ":=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Take", "[", 
         RowBox[{"#", ",", " ", 
          RowBox[{"UpTo", "[", "3", "]"}]}], "]"}], ")"}], " ", "&"}], " ", "/@",
       " ", "wordsInLang"}]}], ";", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
      "If", " ", "language", " ", "A", " ", "has", " ", "NA", " ", 
       "translations", " ", "for", " ", "word", " ", "X", " ", "and", " ", 
       "language", " ", "B", " ", "has", " ", "NB", " ", "translations"}], 
      ",", " ", 
      RowBox[{"then", " ", "it", " ", "will", " ", "take", " ", "us", " ", 
       RowBox[{"(", 
        RowBox[{"NA", "*", "NB"}], ")"}], " ", "expensive", " ", "operations",
        " ", "to", " ", "compute", " ", 
       RowBox[{"distances", ".", " ", "Instead"}], " ", "we", " ", "should", 
       " ", "limit", " ", "the", " ", "number", " ", "of", " ", 
       RowBox[{"variants", "."}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"WordsNormalize", "[", "wordsInLang_List", "]"}], " ", ":=", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"StringDelete", "[", 
         RowBox[{"#", ",", " ", 
          RowBox[{"Except", "[", "WordCharacter", "]"}]}], "]"}], ")"}], " ", 
       "&"}], " ", "/@", " ", 
      RowBox[{"ToLowerCase", " ", "/@", " ", 
       RowBox[{"RemoveDiacritics", " ", "/@", " ", 
        RowBox[{"Transliterate", " ", "/@", " ", "wordsInLang"}]}]}]}]}], 
    ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"The", " ", "similarity", " ", "metrics"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsDissimilarity", "[", 
      RowBox[{"wordA_String", ",", " ", "wordB_String"}], "]"}], " ", ":=", 
     " ", 
     RowBox[{
      RowBox[{"DamerauLevenshteinDistance", "[", 
       RowBox[{"wordA", ",", " ", "wordB"}], "]"}], " ", "/", " ", 
      RowBox[{"Max", "[", 
       RowBox[{
        RowBox[{"StringLength", "[", "wordA", "]"}], ",", " ", 
        RowBox[{"StringLength", "[", "wordB", "]"}]}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsDissimilarity", "[", 
      RowBox[{"wordA_List", ",", " ", "wordB_List"}], "]"}], " ", ":=", " ", 
     RowBox[{"WordsDissimilarity", "[", 
      RowBox[{
       RowBox[{
       "wordA", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], ",", 
       " ", 
       RowBox[{
       "wordB", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsPairDissimilarity", "[", "wordsPair_", "]"}], " ", ":=", 
     " ", 
     RowBox[{"WordsDissimilarity", "[", 
      RowBox[{
       RowBox[{
       "wordsPair", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       ",", " ", 
       RowBox[{
       "wordsPair", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"WordsSetsDissimilarity", "[", 
      RowBox[{"wordsA_List", ",", " ", "wordsB_List"}], "]"}], " ", ":=", " ", 
     RowBox[{"Min", "[", 
      RowBox[{"Map", "[", 
       RowBox[{"WordsPairDissimilarity", ",", " ", 
