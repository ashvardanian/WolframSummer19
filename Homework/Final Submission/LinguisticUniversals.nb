          "both", " ", 
         RowBox[{"languages", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
       "\t", 
       RowBox[{
        RowBox[{"sharedInEng", " ", "=", " ", 
         RowBox[{"WordsTranslatedToBoth", "[", 
          RowBox[{"langIdxA", ",", " ", "langIdxB"}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\t", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Then", " ", "we", " ", "need", " ", "to", " ", "translate", " ", 
          "those", " ", "2", " ", "separately", " ", "and", " ", "find", " ", 
          "how", " ", "close"}], " ", "*)"}], "\[IndentingNewLine]", "\t", 
        RowBox[{"translatedToA", " ", "=", " ", 
         RowBox[{"WordsNormalize", " ", "@", " ", 
          RowBox[{"WordsLimit", "@", " ", 
           RowBox[{"WordsTranslateTo", "[", 
            RowBox[{"sharedInEng", ",", " ", "langIdxA"}], "]"}]}]}]}], ";", 
        "\[IndentingNewLine]", "\t", 
        RowBox[{"translatedToB", " ", "=", " ", 
         RowBox[{"WordsNormalize", " ", "@", " ", 
          RowBox[{"WordsLimit", "@", " ", 
           RowBox[{"WordsTranslateTo", "[", 
            RowBox[{"sharedInEng", ",", " ", "langIdxB"}], "]"}]}]}]}], ";", 
        "\[IndentingNewLine]", "\t", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "[", "translatedToA", "]"}], " ", "\[NotEqual]", 
           " ", 
           RowBox[{"Length", "[", "translatedToB", "]"}]}], ",", " ", 
          RowBox[{"Throw", "[", 
           RowBox[{"Null", ",", " ", "\"\<Not all translations found!\>\""}], 
           "]"}]}], "]"}], ";", "\[IndentingNewLine]", "\t", 
        RowBox[{"distancesPerTranslatin", " ", "=", " ", 
         RowBox[{"MapThread", "[", 
          RowBox[{"WordsSetsDissimilarity", ",", " ", 
           RowBox[{"{", 
            RowBox[{"translatedToA", ",", "translatedToB"}], "}"}]}], "]"}]}],
         ";", "\[IndentingNewLine]", "\t", 
        RowBox[{"1", " ", "-", " ", 
         RowBox[{"Mean", "[", "distancesPerTranslatin", "]"}]}]}]}], 
